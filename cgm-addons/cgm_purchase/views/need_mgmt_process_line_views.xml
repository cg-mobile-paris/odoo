<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="need_mgmt_process_line_tree_view" model="ir.ui.view">
            <field name="name">need_mgmt_process_line_tree_view</field>
            <field name="model">need.mgmt.process.line</field>
            <field name="arch" type="xml">
                <tree string="Need Mgmt Process Lines" editable="bottom">
                    <field name="currency_id" invisible="1"/>
                    <field name="sequence" widget="handle"/>
                    <field name="product_id" string="SKU Code"/>
                    <field name="name"/>
                    <field name="image" widget="image" class="oe_avatar"/>
                    <field name="barcode" optional="hide"/>
                    <field name="physical_qty" sum="Total Physical Qty"/>
                    <field name="outgoing_qty" sum="Total Outgoing Qty"/>
                    <field name="available_qty" sum="Total Available Qty"/>
                    <field name="incoming_qty" sum="Total Incoming Qty"/>
                    <field name="projected_qty" sum="Total Projected Qty"/>
                    <field name="product_qty" sum="Total Ordered Qty"/>
                    <field name="price_unit" widget="monetary" options="{'currency_field': 'currency_id'}"/>
                    <field name="seller_id" optional="hide"/>
                </tree>
            </field>
        </record>


    </data>
</odoo>